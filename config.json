{
  "name": "RTL_433 to MQTT Bridge",
  "version": "0.2",
  "slug": "rtl4332mqtt",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/csacca/rtl4332mqtt",
  "startup": "before",
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "host_network": "False",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "options": {
    "mqtt_host": "hassio.local",
    "mqtt_port": 8883,
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "homeassistant/rtl443",
    "protocol": 100,
    "frequency": 433920000,
    "gain": 60,
    "frequency_offset": 350,
    "rtl_serial": 10000002
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "int",
    "frequency": "int",
    "gain": "int",
    "frequency_offset": "int",
    "rtl_serial": "int"
  }
}
